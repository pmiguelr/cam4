function extractCDATA(element) {
    if (element) {
        for (var i = 0; i < element.childNodes.length; i++) {
            var nodeType = element.childNodes[i].nodeType;
            // fix for IE
            // 4 CDATA nodeType
            // 8 COMMENT nodeType
            if (nodeType == 4 || nodeType == 8) {
                return element.childNodes[i].data.replace(/\s*\[CDATA\[/, '').replace(/\]\]\s*/, '');
            }
        }
    }
    return '';
}

