            var calls = 0;

            function startCheck() {
                $j.ajax({
                    url: '/verify/status?username=' + userName + '&_=' + (new Date()).getTime(),
                    type: 'GET',
                    error: function (err, status, errThrown) {

                        calls++;
                        if (calls < 500) {
